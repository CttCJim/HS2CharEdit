    <Canvas Background="White">
        <Menu Width="800">
            <MenuItem Header="_Card">
                <!--<MenuItem Header="_New" />-->
                <MenuItem Header="_Open" Click="btnOpenFile_Click"/>
                <MenuItem Header="_Save" Click="btnSaveFile_Click"/>
                <Separator />
                <MenuItem Header="_Exit" />
            </MenuItem>
            <MenuItem Header="_Image Replacer">
                <MenuItem Header="_Load New PNG" Click="loadNewPNG" />
                <MenuItem Header="_Restore Original PNG" Click="loadOldPNG" />
            </MenuItem>
            <MenuItem Header="_Help">
                <MenuItem Header="_About HS2CharEdit" Click="showAbout"/>
                <MenuItem Header="User _Guide" />
                <MenuItem Header="_Support the Creator" />
            </MenuItem>
        </Menu>


